    "    pixels = ['r','g','infra1','infra2']\n",
    "    \n",
    "    temp = list(product(positions, pixels))\n",
    "    \n",
    "    colnames = list()\n",
    "    \n",
    "    for name in temp:\n",
    "        colnames.append(name[0]+name[1])\n",
    "\n",
    "    colnames.append('land_type')\n",
    "    \n",
    "    df.columns = colnames\n",
    "    \n",
    "    return df"
   ]
